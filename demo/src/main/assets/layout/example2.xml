<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_config_my_empty"
    android:background="#aa000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/dialog"
        android:layout_centerInParent="true"
        android:orientation="vertical"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
    	android:background="#aaffffff">
        <LinearLayout
            android:padding="20dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:gravity="center"
                android:textColor="@color/color_config_text"
                android:textSize="16sp"
                android:layout_margin="5dp"
                android:text="@string/config_share_mine_del_tip"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>
        <View
            android:layout_margin="2dp"
            android:background="#29352d"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_height="40dp">
            <Button
                android:id="@+id/btn_config_share_del_cancel"
                android:text="@string/config_share_login_cancel"
                android:textColor="@color/color_config_text"
                android:textSize="14sp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="40dp" />
            <View
                android:background="#29352d"
                android:layout_width="1dp"
                android:layout_height="38dp"/>
            <Button
                android:id="@+id/btn_config_share_del_ok"
                android:text="@string/ok"
                android:textColor="@color/color_config_active"
                android:textSize="14sp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="40dp" />
        </LinearLayout>
    </LinearLayout>
    <include layout="@layout/example"
        android:id="@+id/fl_menu"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
    />

</RelativeLayout>